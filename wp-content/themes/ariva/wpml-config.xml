<wpml-config>
    <custom-fields>
        <custom-field action="translate">themestudio_portfolio_type </custom-field>
        <custom-field action="translate">themestudio_portfolio_image </custom-field>
        <custom-field action="translate">themestudio_portfolio_slider </custom-field>
        <custom-field action="translate">themestudio_portfolio_video </custom-field>
        <custom-field action="translate">themestudio_portfolio_soundcloud </custom-field>
        <custom-field action="translate">themestudio_title_meta </custom-field>
        <custom-field action="translate">themestudio_portfolio_url </custom-field>
        <custom-field action="translate">themestudio_portfolio_hover_color </custom-field>
        <custom-field action="translate">themestudio_portfolio_single_layout </custom-field>
        <custom-field action="translate">themestudio_service_select_icon </custom-field>
        <custom-field action="translate">themestudio_feature_select_icon </custom-field>
        <custom-field action="translate">themestudio_feature_type </custom-field>
        <custom-field action="translate">themestudio_feature_image </custom-field>
        <custom-field action="translate">themestudio_feature_slider </custom-field>
        <custom-field action="translate">themestudio_feature_video </custom-field>
        <custom-field action="translate">themestudio_feature_soundcloud </custom-field>
        <custom-field action="translate">themestudio_testimonials_name </custom-field>
        <custom-field action="translate">themestudio_testimonials_position </custom-field>
        <custom-field action="translate">themestudio_image_gallery</custom-field>
        <custom-field action="translate">themestudio_post_image </custom-field>
        <custom-field action="translate">themestudio_post_video_embed </custom-field>
        <custom-field action="translate">themestudio_post_audio_embed </custom-field>
        <custom-field action="translate">themestudio_quote_content </custom-field>
        <custom-field action="translate">themestudio_quote_author </custom-field>
        <custom-field action="translate">themestudio_link_description </custom-field>
        <custom-field action="translate">themestudio_link_url </custom-field>
        <custom-field action="translate">themestudio_custom_page_title </custom-field>
        <custom-field action="translate">themestudio_custom_page_description </custom-field>
        <custom-field action="translate">themestudio_show_banner </custom-field>
        <custom-field action="translate">themestudio_theme_options</custom-field>
        <custom-field action="translate">themestudio_bg_color </custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">portfolio</custom-type>
        <custom-type translate="1">testimonial</custom-type>
        <custom-type translate="1">feature</custom-type>
        <custom-type translate="1">service</custom-type>
        <custom-type translate="0">megamenu</custom-type>
    </custom-types>    
    <taxonomies>
        <taxonomy translate="1">portfolio_cats</taxonomy>
        <taxonomy translate="1">feature_cats</taxonomy>
    </taxonomies>    
    <admin-texts>
        <key name="ariva">
            <key name="general_introduction" />
                <key name="ts-logo" />
                <key name="ts-blog-title" />
                <key name="ts-blog-sub-title" />
                <key name="ts-blog-reading-text" />
                <key name="ts-title-subscribe" />
                <key name="ts-phone-subscribe" />
                <key name="ts-website-subscribe" />
                <key name="portfolio_title" />
                <key name="portfolio_sub_title" />
                <key name="footer_copyright_text" />
        </key>
        <key name="simple_string_option" />
    </admin-texts>    
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>    
        <key name="icl_language_switcher_sidebar">0</key>  
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>    
    </language-switcher-settings>    
</wpml-config>


